{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"type": "shell",
			"command": "go build -o bin/local/handler",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": ["$go"]
		},
		{
			"type": "func",
			"label": "func: host start",
			"command": "host start",
			"problemMatcher": "$go",
			"isBackground": true,
			"dependsOn": "build"
		}
	]
}